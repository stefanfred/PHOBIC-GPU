ivec2 compareComb(uint n, uint N) {
    //return pairs[n];
    if ((N&1)==1){
        uint x=(n*2+1)%N;
        uint y=(n*2+1)/N;
        if (x<y) {
            y+=1;
        }
        return ivec2(x,y);
    } else {
        uint x = (n * 2 + 1) % (N - 1);
        uint y = (n * 2 + 1) / (N - 1);
        if (x > y) {
            x += 1;
        }
        return ivec2(x, y);
    }
}